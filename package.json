{
  "name": "quid-pro-quo",
  "version": "0.0.164",
  "description": "JS Library for building web servers using pure functions and generators.",
  "main": "index.js",
  "scripts": {
    "link": "npm exec --ws -- npm link",
    "build": "npm run build --ws",
    "clean": "npm run clean --ws",
    "login": "npm login",
    "publish": "npm publish --ws",
    "patch": "npm version patch --ws && npx tsx ./scripts/applyPatch.ts",
    "go": "npm run patch && npm run build && npm run publish",
    "lint": "npm run lint --ws",
    "lint:fix": "npm run lint:fix --ws",
    "format": "npm run format --ws",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:types": "tsc --build tsconfig.test.json",
    "watch-all": "npx npm-run-all --parallel watch:*",
    "watch:quidproquo-core": "npm run watch -w quidproquo-core",
    "watch:quidproquo-webserver": "npm run watch -w quidproquo-webserver",
    "watch:quidproquo-config-aws": "npm run watch -w quidproquo-config-aws",
    "watch:quidproquo-actionprocessor-js": "npm run watch -w quidproquo-actionprocessor-js",
    "watch:quidproquo-actionprocessor-node": "npm run watch -w quidproquo-actionprocessor-node",
    "watch:quidproquo-actionprocessor-awslambda": "npm run watch -w quidproquo-actionprocessor-awslambda",
    "watch:quidproquo-neo4j": "npm run watch -w quidproquo-neo4j",
    "watch:quidproquo-deploy-webpack": "npm run watch -w quidproquo-deploy-webpack",
    "watch:quidproquo-deploy-awscdk": "npm run watch -w quidproquo-deploy-awscdk",
    "watch:quidproquo-web": "npm run watch -w quidproquo-web",
    "watch:quidproquo-actionprocessor-web": "npm run watch -w quidproquo-actionprocessor-web",
    "watch:quidproquo-web-react": "npm run watch -w quidproquo-web-react",
    "watch:quidproquo-web-admin": "npm run watch -w quidproquo-web-admin",
    "watch:quidproquo-dev-server": "npm run watch -w quidproquo-dev-server",
    "watch:quidproquo": "npm run watch -w quidproquo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe-coady/quidproquo.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joe-coady/quidproquo/issues"
  },
  "homepage": "https://github.com/joe-coady/quidproquo#readme",
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "quidproquo-tsconfig",
    "quidproquo-eslint-config",
    "quidproquo-core",
    "quidproquo-webserver",
    "quidproquo-config-aws",
    "quidproquo-actionprocessor-js",
    "quidproquo-actionprocessor-node",
    "quidproquo-actionprocessor-awslambda",
    "quidproquo-neo4j",
    "quidproquo-deploy-webpack",
    "quidproquo-deploy-awscdk",
    "quidproquo-web",
    "quidproquo-actionprocessor-web",
    "quidproquo-web-react",
    "quidproquo-web-admin",
    "quidproquo-testing",
    "quidproquo-dev-server",
    "quidproquo"
  ],
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.21.0",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}